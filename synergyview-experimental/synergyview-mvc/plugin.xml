<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="synergyviewmvc.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="synergyviewmvc.Perspective"
            id="synergyviewmvc.perspective">
      </perspective>
   </extension>
      <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="synergyviewmvc.application"
            name="%appName">
         <property
               name="windowImages"
               value="icons/bomb.png">
         </property>
          <property
                name="aboutText"
                value="%aboutText">
         </property>
         <property
               name="appName"
               value="%appName">
         </property>
         <property
               name="aboutImage"
               value="product_lg.gif">
         </property>
         <property
               name="startupProgressRect"
               value="33,270,402,7">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customisation.ini">
         </property>
         <property
               name="startupForegroundColor"
               value="808080">
         </property>
         <property
               name="startupMessageRect"
               value="33,250,402,15">
         </property>
      </product>
   </extension>
   
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="synergyviewmvc.projects.projectexplorerview">
         <includes>
            <contentExtension
                  pattern="synergyviewmvc.projects.navigatorContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
         <navigatorContent
               activeByDefault="true"
               contentProvider="synergyviewmvc.navigation.NavigatorContentProvider"
               id="synergyviewmvc.projects.navigatorContent"
               labelProvider="synergyviewmvc.navigation.NavigatorLabelProvider"
               name="Projects">
               <enablement>
		         <or>         
		            <instanceof 
		                  	value="uk.ac.durham.tel.commons.jface.node.INode" /> 
		         </or>
         </enablement>
         
               <dropAssistant
                     class="synergyviewmvc.projects.ProjectsDropAdapterAssistant"
                     id="synergyviewmvc.projects.dropAssistant">
                     
                  <possibleDropTargets>
                   <or>
                 	 <adapt type="synergyviewmvc.navigation.model.INode" /> 
                  	</or>
                  </possibleDropTargets>
                  
               </dropAssistant>
               
         </navigatorContent>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.views">
      <view
            class="synergyviewmvc.projects.ui.ProjectExplorerViewPart"
            id="synergyviewmvc.projects.projectexplorerview"
            name="%synergyviewmvc.projectexplorerview"
            category="org.eclipse.ui">
            
      </view>
      <view
            class="synergyviewmvc.attributes.ui.CodingExplorerViewPart"
            id="synergyviewmvc.attributes.ui.CodingExplorerViewPart"
            name="Coding Explorer"
