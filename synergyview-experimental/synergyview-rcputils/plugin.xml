<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
         point="org.eclipse.ui.commands">
      <command
            name="Check for Updates"
            id="synergyview.update">
      </command>
      <command
            name="Install New Software..."
            id="synergyview.install">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="synergyview.update"
            class="synergyviewrcputils.handler.UpdateHandler">
      </handler>
      <handler
            commandId="synergyview.install"
            class="synergyviewrcputils.handler.InstallNewSoftwareHandler">
      </handler>
   </extension>
</plugin>
